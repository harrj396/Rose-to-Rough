<!-- Loop over each object in the posts array -->
{{#each review as |review| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/project/{{review.id}}">{{review.name}}</a>
    </h2>
    <p>
      <span for="img" aria-label="bottle">üçæ</span>
      <!-- Pass needed_funding value to the helper function -->
      <span class="">{{}}</span>
      Needed!
    </p>
    <p>Created by {{review.user.name}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{review.description}}
    </p>
  </div>
</div>
{{/each}}

